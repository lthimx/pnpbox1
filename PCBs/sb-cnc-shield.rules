
(rules PCB sb-cnc-shield
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 6848)
    (layer_rule F.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.1)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.9)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:400_um-5V" "Via[0-1]_600:400_um" 5V
  )
  (via 
    "Via[0-1]_600:400_um-5V_pi" "Via[0-1]_600:400_um" "5V_pi"
  )
  (via 
    "Via[0-1]_600:400_um-DRV" "Via[0-1]_600:400_um" DRV
  )
  (via 
    "Via[0-1]_600:400_um-GND" "Via[0-1]_600:400_um" GND
  )
  (via 
    "Via[0-1]_600:400_um-POWER" "Via[0-1]_600:400_um" POWER
  )
  (via 
    "Via[0-1]_600:400_um-mot" "Via[0-1]_600:400_um" mot
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (via_rule
    5V "Via[0-1]_600:400_um-5V"
  )
  (via_rule
    "5V_pi" "Via[0-1]_600:400_um-5V_pi"
  )
  (via_rule
    DRV "Via[0-1]_600:400_um-DRV"
  )
  (via_rule
    GND "Via[0-1]_600:400_um-GND"
  )
  (via_rule
    POWER "Via[0-1]_600:400_um-POWER"
  )
  (via_rule
    mot "Via[0-1]_600:400_um-mot"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    RESET "Net-(U2-Pad5)" "Net-(U3-Pad5)" "Net-(U4-Pad5)" "Net-(U5-Pad5)" EN XSTEP XDIR
    YSTEP YDIR ZSTEP ZDIR ZLIMIT YLIMIT XLIMIT "SPIN_DIR"
    "SPIN_EN" +3V3 "Net-(U1-Pad23)" "Net-(U1-Pad16)" "_SCL_" "_SDA_" DC res
    /G "B_led" "O_led" "O_sig" "Net-(conn26_1-Pad25)" "Net-(conn26_1-Pad26)" "Net-(MSA1-Pad2)" "Net-(MSA1-Pad4)"
    "Net-(MSA1-Pad6)" "Net-(MSX1-Pad6)" "Net-(MSX1-Pad4)" "Net-(MSX1-Pad2)" "Net-(MSY1-Pad2)" "Net-(MSY1-Pad4)" "Net-(MSY1-Pad6)" "Net-(MSZ1-Pad6)"
    "Net-(MSZ1-Pad4)" "Net-(MSZ1-Pad2)" "B_sig" "pump_ctrl" "up_light" D "Net-(Rstp1-Pad2)" "Net-(RstpX1-Pad2)"
    "Net-(RstpY1-Pad2)" "Net-(RstpZ1-Pad2)" "Net-(Pled1-Pad2)" "Net-(Rpwr1-Pad2)" "Net-(Rpwr_1-Pad2)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class 5V
    +5V VIN "VIN_" Vin
    (clearance_class 5V)
    (via_rule 5V)
    (rule
      (width 700.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "5V_pi"
    "5V_pi"
    (clearance_class "5V_pi")
    (via_rule 5V_pi)
    (rule
      (width 700.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class DRV
    (clearance_class DRV)
    (via_rule DRV)
    (rule
      (width 700.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class GND
    GND GNDA
    (clearance_class GND)
    (via_rule GND)
    (rule
      (width 700.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class POWER
    (clearance_class POWER)
    (via_rule POWER)
    (rule
      (width 800.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class mot
    y2B x2B y2A x2A y1A x1A y1B x1B
    a2B z2B a2A z2A a1A z1A a1B z1B
    (clearance_class mot)
    (via_rule mot)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)