
(rules PCB fixed_head_pcb
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias off)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 113)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule B.Cu
      (active off)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 400.0)
    (clear 400.2)
    (clear 200.0 (type smd_to_turn_gap))
    (clear 100.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-alim" "Via[0-1]_800:400_um" alim
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    alim "Via[0-1]_800:400_um-alim"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    A1A A2A A1B A2B Z1A Z2A Z1B Z2B
    pwm limZ SCL SDA
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class alim
    +12V +3V3 GND
    (clearance_class alim)
    (via_rule alim)
    (rule
      (width 700.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)